#include <iostream>
using namespace std;

int n=0,a;
int main()
{
     cout<<"ingrese un numero"<<endl;
     cin>>a;
     for(int i=1;i<(a+1);i++)
    {
      if(a%i==0)
       {
         n++;
         }
      }
      if(n==2)
       {
         cout<<"no es primo";
         }
      else
       {
         cout<<"es primo";
         }
     return 0;
    }
